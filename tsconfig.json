{
  "compilerOption": {
    "target": "esnext",
    "types": ["node", "express"],
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true
  },
  "exclude": ["node_modules", "client"],
  "include": ["**/*"]
}
